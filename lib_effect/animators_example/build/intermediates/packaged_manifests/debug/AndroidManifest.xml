<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xxf.effect.recyclerview.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="31" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.xxf.effect.recyclerview.demo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.xxf.effect.recyclerview.demo.AnimatorSampleActivity" />
        <activity android:name="com.xxf.effect.recyclerview.demo.AdapterSampleActivity" />

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="com.xxf.effect.recyclerview.demo.com.squareup.picasso"
            android:exported="false" />
    </application>

</manifest>